{
    "name": "your-vendor/pay2house-laravel",
    "description": "Laravel package for Pay2.House API integration",
    "type": "library",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "YourVendor\\Pay2House\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YourVendor\\Pay2House\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YourVendor\\Pay2House\\Pay2HouseServiceProvider"
            ],
            "aliases": {
                "Pay2House": "YourVendor\\Pay2House\\Facades\\Pay2House"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
